generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(uuid())
  name         String
  username     String   @unique
  password     String
  creationDate DateTime @default(now())
  updateDate   DateTime @updatedAt
}
