{
  "name": "@yet-another-todo-app/yet-another-todo-app-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "yet-another-todo-app-e2e",
    "implicitDependencies": [
      "yet-another-todo-app"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/yet-another-todo-app-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "yet-another-todo-app:build"
        ]
      }
    }
  }
}
